			"description": "Shows how much time you've played on a world.",
			"contributors": {
				"lemonjamesd": "Owner"
			},
			"contact": {
				"homepage": "https://example.com/",
				"issues": "https://github.com/QuiltMC/quilt-template-mod/issues",
				"sources": "https://github.com/QuiltMC/quilt-template-mod"
			},
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "io.github.lemonjamesd.TimePlayed"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
